$("<%= escape_javascript(render "earning_history") %>").appendTo("#earning-histories");
$(".user-count").text("<%= t("dashboard.earning_history.showing_earning", number: @total_earning_histories_count) %>")

<% if @earning_histories.total_pages == @earning_histories.current_page %>
  $("#load-more-earning-histories").hide();
<% else %>
  $("#load-more-earning-histories").attr("href", "<%== earning_histories_path(request.params.except(:controller,:action,:locale).merge(page: (@earning_histories.current_page+1))) %>");
<% end %>
