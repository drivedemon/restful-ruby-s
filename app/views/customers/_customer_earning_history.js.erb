$("<%= escape_javascript(render "customer_earning_history_data", earning_histories: @earning_histories) %>").appendTo("#earning-histories");

<% if @earning_histories.total_pages == @earning_histories.current_page %>
  $("#load-more-earning-histories").hide();
<% else %>
  $("#load-more-earning-histories").attr("href", "<%== earning_customers_path(request.params.except(:controller,:action,:locale).merge(page: @earning_histories.current_page+1)) %>");
<% end %>
